{
  "ConnectionStrings": {
    "DatabaseContext": ""
  },
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Warning",
      "System.Net.Http.HttpClient": "Information",
      "OpenTelemetry": "Information"
    }
  },
  "AllowedHosts": "*",
  "CorsSettings": {
    "AllowedOrigins": [ "http://localhost:5173", "http://ai.receipts.web" ]
  },
  "OllamaSettings": {
    "Url": "http://ollama:11434",
    "VisionModel": "llama3.2-vision",
    "TimeoutFromMinutes": 5,
    "ContextWindowSize": 8192
  },
  "DetailedErrors": true,
  "EntityFrameworkSettings": {
    "BatchSize": 50000,
    "CommandTimeout": 30,
    "BulkRowCount": 50000,
    "MaxRetryCount": 3,
    "MaxRetryDelay": 5
  },
  "SeqSettings": {
    "ServerUrl": "http://seq/ingest/otlp/v1/traces",
    "ApiKey": "K1eGc3g3YUo3HgBVXWGg"
  },
  "FileStorage": {
    "UploadPath": "/app/uploads",
    "MaxFileSizeBytes": 10485760
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.Seq" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Information"
      }
    },
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://seq:5341"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
    "Properties": {
      "Application": "ai.receipts"
    }
  }
}
